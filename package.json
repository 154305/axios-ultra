{
  "name": "axios-ultra",
  "version": "0.0.2",
  "author": "by zhizhuo <1543057945@qq.com> (https://github.com/154305)",
  "description": "一个全平台请求库 支持web、taro、uniapp、小程序,并且支持通用业务拦截器",
  "keywords": "axios,uniapp,taro,request,xhr,web",
  "homepage": "https://github.com/154305/axios-ultra",
  "repository": "154305/axios-ultra",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "install:examples": "cd examples/web & yarn & cd ../taro & yarn",
    "build": "tsup",
    "ps": "node ./scripts/publish.ts",
    "start:web": "cd examples/web & npm run dev",
    "start:taro": "cd examples/taro & npm run dev:weapp"
  },
  "dependencies": {
    "lodash-es": "^4.17.6",
    "axios": "^0.27.2"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@tarojs/taro": "^3.4.1",
    "@dcloudio/types": "^2.6.4",
    "tsup": "^6.2.1",
    "unbuild": "^0.7.6"
  }
}